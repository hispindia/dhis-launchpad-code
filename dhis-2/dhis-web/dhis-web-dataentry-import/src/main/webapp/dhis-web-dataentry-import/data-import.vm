<script type="text/javascript" src="javascript/jasny-bootstrap.js"></script>
<link href="style/jasny-bootstrap.css" rel="stylesheet">
<link href="style/bootstrap.css" rel="stylesheet">
<link href="style/style.css" rel="stylesheet">

<script type="text/javascript" src="javascript/importData.js"></script>
<script type="text/javascript" src="javascript/xlsx.js"></script>

<div class="container" id="mainPage">
    <h1>Excel Import </h1>
    <div class="row">
        <div class=" col-md-6 col-sm-6  col-md-offset-0 report-type-container">
            <h2><small><i class="fa fa-list-alt fa-2x black"></i></small><span class="horizonal-spacing"></span></h2>
            <div class="form-group">

                <div class="fileinput fileinput-new input-group" data-provides="fileinput" id="fileinput_id">
                    <div class="form-control" data-trigger="fileinput"><i
                            class="glyphicon glyphicon-file fileinput-exists"></i> <span class="fileinput-filename"></span>
                    </div>
                <span class="input-group-addon btn btn-default btn-file"><span class="fileinput-new">Select file</span><span class="fileinput-exists">Change</span><input
                        type="file" name="..." id="fileInput" onchange="" accept=".xlsx" ></span>
                    <a href="#" class="input-group-addon btn btn-default fileinput-exists"
                       data-dismiss="fileinput">Remove</a>
                </div>

                <button class="btn btn-success vertical-spacing" type="submit" id="import" onclick="uploadFile()">Import</button>
                <button class="btn btn-primary vertical-spacing horizonal-spacing" type="submit" id="cancel" onclick="cancel()">Cancel</button>
            </div>
        </div>
    </div>
    <div id="contentDiv"></div>
</div>