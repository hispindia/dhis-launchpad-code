<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
"-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
"http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>

  <include file="dhis-web-commons.xml" />

  <package name="dhis-web-dataentry-import" extends="dhis-web-commons" namespace="/dhis-web-dataentry-import">

    <action name="index" class="org.hisp.dhis.edi.action.NoAction">
      <result name="success" type="velocity">/main.vm</result>
      <param name="page">/dhis-web-dataentry-import/data-import.vm</param>
      <param name="javascripts">javascript/moment.js</param>
      <param name="stylesheets">style/dhis-web-dataentry-import.css</param>
    </action>

     <action name="importData" class="org.hisp.dhis.edi.action.DataImportAction">
      <param name="page">/dhis-web-dataentry-import/import.vm</param>
	  <result name="success" type="velocity">/content.vm</result>
      <param name="javascripts">javascript/moment.js</param>
    </action>

  </package>
</struts>
