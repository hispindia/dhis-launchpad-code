<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd"    
    >

<hibernate-mapping package="org.hisp.dhis.dxf2.sm.api">
  <class name="InstanceBusinessRules" table="instance_businessrules">

    <id name="id" column="businessruleid">
      <generator class="native" />
    </id>
    
    <property name="approvalUserGroupUid" column="appusergroupuid" />
    
    <property name="acceptanceUserGroupUid" column="accusergroupuid" />

    <many-to-one name="instance" lazy="false" class="org.hisp.dhis.dxf2.sm.api.SynchInstance" column="instanceid"
      not-null="true" foreign-key="fk_businessrules_instanceid" />

  </class>
</hibernate-mapping>
