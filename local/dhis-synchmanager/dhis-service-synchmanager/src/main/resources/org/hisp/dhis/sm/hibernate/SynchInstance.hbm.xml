<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd"
    [<!ENTITY identifiableProperties SYSTEM "classpath://org/hisp/dhis/common/identifiableProperties.hbm">]
    >

<hibernate-mapping package="org.hisp.dhis.dxf2.sm.api">
  <class name="SynchInstance" table="instanceconfig">

    <id name="id" column="instanceid">
      <generator class="native" />
    </id>
    
    &identifiableProperties;

    <property name="name" column="name" not-null="true"  length="120" />

    <property name="url" column="url" not-null="true"  />

    <property name="userId" column="userid" not-null="true" />

    <property name="password" column="password" not-null="true" />

    <property name="synchType" column="synchtype" not-null="true" />
    
    <property name="type" column="type" not-null="true" />

  </class>
</hibernate-mapping>
