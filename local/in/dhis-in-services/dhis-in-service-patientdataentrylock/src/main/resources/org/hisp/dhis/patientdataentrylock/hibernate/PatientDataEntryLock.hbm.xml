<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
 "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
 "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
  <class name="org.hisp.dhis.patientdataentrylock.PatientDataEntryLock" table="patientdataentrylock">

   <id name="id" column="dataentrylockid">
      <generator class="native" />
    </id>
    
     <!-- OrganisationUnit -->
        
    <many-to-one name="organisationUnit" class="org.hisp.dhis.organisationunit.OrganisationUnit" column="organisationunitid"
        not-null="true" foreign-key="fk_patientdataentrylock_organisationunitid" />   
	
	<!-- Period -->	
    <many-to-one name="period" class="org.hisp.dhis.period.Period" column="periodid"
      not-null="true" foreign-key="fk_patientdataentrylock_periodid" />			
	
	<!-- Patient -->    			
    <many-to-one name="patient" class="org.hisp.dhis.patient.Patient" column="patientid"
      not-null="true" foreign-key="fk_patientdataentrylock_patientid" />
	
	<!-- Lock -->    
	<property name="lockStatus"  column="lockstatus" not-null="true" />
	
  </class>
</hibernate-mapping>
