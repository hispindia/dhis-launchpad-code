				        <xmlTemplateName>nwfgorbd.xml</xmlTemplateName>
					<displayName>MISM Weekly Offtake Report</displayName>
					<periodType>Daily</periodType>
                                        <checkerTemplateName>Off.T.31(Ka).xls</checkerTemplateName>
					<checkerRangeForHeader>0,1-0,1</checkerRangeForHeader>
					<checkerRangeForData>48,0-70,0</checkerRangeForData>
                                        <sheetNo>0</sheetNo>
				</BDImportSheet>

                                 <BDImportSheet>				
